<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.happyhour">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/Theme.HappyHour">
        <activity
            android:name=".activities.Activity_create_bar"
            android:exported="false" />
        <activity
            android:name=".activities.Activity_bar_details"
            android:exported="false" />
        <activity
            android:name=".activities.Activity_registration_private_account"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.Activity_private_account"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.Activity_bar_account"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.Activity_login"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.Activity_user_connect"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.Activity_enter_app"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>